<%= render 'info', user: @user %>
<h1>Users</h1>
 <table>
   <thead>
     <tr>
       <th>Image</th>
       <th>Name</th>
       <th></th>
     </tr>
   </thead>
    <tbody>
      <% @users.each do |user| %>
       <tr>
         <td><%= image_tag user.get_profile_image(50,50) %></td>
         <td><%= user.name %></td>
         <td><%= link_to "show", user_path(user.id) %></td>
       </tr>
      <% end %>
    </tbody>
 </table>


  